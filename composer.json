{
  "name": "codexten/yii-omnipay",
  "description": "php hosts file manager",
  "keywords": [
    "hosts",
    "codexten"
  ],
  "type": "library",
  "require": {
    "omnipay/common": "*"
  },
  "autoload": {
    "psr-4": {
      "codexten\\yii\\omnipay\\": "src"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.0.x-dev"
    },
    "config-plugin": {
      "omnipay": "config/omnipay.php"
    }
  }
}
